{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.17.2",
    "axios": "^0.25.0",
    "craco-less": "^1.20.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "cross-env PORT=3006 craco start",
    "build": "craco build",
    "deploy": "aws s3 sync build/ s3://qualicorp-site --acl public-read",
